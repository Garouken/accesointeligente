<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style src="RequestSearchView.css" />
	<g:HTMLPanel>
		<g:Label>Busqueda</g:Label>
		<g:HTMLPanel>
			<g:TextBox ui:field="keyWord"/>
			<g:HTMLPanel>
				<g:Label>Organismo</g:Label>
				<g:SuggestBox ui:field="institution" />
			</g:HTMLPanel>
			<g:HTMLPanel>
				<g:Label>Fecha</g:Label>
				<g:Label>Desde</g:Label>
				<c:DateBox ui:field="minDate"></c:DateBox>
				<g:Label>Hasta</g:Label>
				<c:DateBox ui:field="maxDate"></c:DateBox>
			</g:HTMLPanel>
			<g:HTMLPanel>
				<g:Label>ESTADO</g:Label>
				<g:CheckBox ui:field="statusPending">Pendiente</g:CheckBox>
				<g:CheckBox ui:field="statusClosed">Respondido</g:CheckBox>
				<g:CheckBox ui:field="statusExpired">Vencido</g:CheckBox>
				<g:CheckBox ui:field="statusDerived">Derivado</g:CheckBox>
			</g:HTMLPanel>
			<g:Button ui:field="search">Buscar</g:Button>
			<g:Button ui:field="clear">Limpiar</g:Button>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>