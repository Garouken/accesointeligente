<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="MainView.css"/>
	<g:HTMLPanel>
		<g:HTMLPanel ui:field="headerPanel" visible="false">
			<g:HTMLPanel>
				<g:MenuBar>
					<g:MenuItem>Mi cuenta
						<g:MenuBar vertical="true">
							<g:MenuItem ui:field="login" visible="false">Ingresar</g:MenuItem>
							<g:MenuItem ui:field="logout" visible="false">Salir</g:MenuItem>
						</g:MenuBar>
					</g:MenuItem>
					<g:MenuItemSeparator/>
					<g:MenuItem ui:field="home">Home</g:MenuItem>
					<g:MenuItemSeparator/>
					<g:MenuItem ui:field="about">Acerca del proyecto</g:MenuItem>
					<g:MenuItemSeparator/>
					<g:MenuItem ui:field="contact">Contacto</g:MenuItem>
				</g:MenuBar>
			</g:HTMLPanel>
		</g:HTMLPanel>
		<g:FlowPanel ui:field="mainPanel" visible="false"/>
		<g:Label ui:field="loginPending" visible="false">Ingresando...</g:Label>
	</g:HTMLPanel>
</ui:UiBinder>
