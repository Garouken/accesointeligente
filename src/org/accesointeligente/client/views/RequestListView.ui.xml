<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style src="RequestListView.css" />
	<g:HTMLPanel addStyleNames="noOverflow">
		<g:Anchor ui:field="requestLink">
			Crea una nueva solicitud
		</g:Anchor>
		<div class="contentPad {style.reqTableContainer}">
		<div class="headSolicitud noMargin"><h1>Listado de solicitudes <!-- este nombre es dinÃ¡mico, indica el contenido del listado--></h1></div>
		<div class=""></div>
			<c:CellTable ui:field="requestTable" pageSize="10" addStyleNames="{style.requestTable}"/>
			<c:SimplePager ui:field="requestPager" rangeLimited="false" addStyleNames="{style.requestPager}"/>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>