<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="HomeView.css"/>
	<g:HTMLPanel>
		<g:FocusPanel ui:field="requestFormLink" visible="false">
			<g:HTMLPanel>
				Haz tu solicitud aquí
			</g:HTMLPanel>
		</g:FocusPanel>
		<g:FocusPanel ui:field="requestListLink">
			<g:HTMLPanel>
				Revisa otras solicitudes aquí
			</g:HTMLPanel>
		</g:FocusPanel>
		<g:FlowPanel ui:field="lastResponses">
			<g:HTMLPanel>
				Ultimas Respuestas
			</g:HTMLPanel>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder>
