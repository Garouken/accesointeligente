<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style src="ResponseWidget.css" />

	<g:HTMLPanel>
		<h3>Respuesta</h3>
		<g:Label ui:field="responseDate" addStyleNames="{style.responseDate}" />
		<pre>
			<g:Label ui:field="responseInfo"></g:Label>
		</pre>
		<br />
		<g:HTMLPanel ui:field="responseAttachmentsPanel">
			<c:CellTable ui:field="attachmentsTable" addStyleNames="{style.downloadTable}" />
		</g:HTMLPanel>
		<br />
		<br />
		<g:FlowPanel ui:field="userResponsePanel"/>
	</g:HTMLPanel>
</ui:UiBinder>